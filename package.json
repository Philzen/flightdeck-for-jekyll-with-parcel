{
  "name": "flightdeck",
  "version": "0.9.9",
  "description": "Flightdeck is an opinionated starter project for Jekyll that uses modern front-end tooling.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "run-p watch:jekyll watch:parcel bs:start",
    "clean": "run-s clean:jekyll clean:depends",
    "watch:jekyll": "bundle exec jekyll server -l -o",
    "clean:jekyll": "run-s bundle exec jekyll clean",
    "clean:depends": "rm -rf node_modules yarn.lock package-lock.json Gemfile.lock"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR"
  ],
  "repository": "https://github.com/ginfuru/Flightdeck.git",
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "Imagemin",
    "Jekyll",
    "Parcel",
    "PostCSS",
    "Sass"
  ],
  "author": "Ginfuru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ginfuru/Flightdeck/issues"
  },
  "homepage": "https://github.com/ginfuru/Flightdeck/",
  "devDependencies": {
    "browser-sync": "^2.24.6",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-imagemin": "^4.0.1",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "stylelint": "^13.7.2",
    "stylelint-prettier": "^1.1.2",
    "svg-sprite-generator": "^0.0.7"
  },
  "dependencies": {}
}
